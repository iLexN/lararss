{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "facade/ignition": "^1.4",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.0",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "myclabs/php-enum": "^1.7",
        "spatie/laravel-queueable-action": "^2.2",
        "thecodingmachine/graphqlite-laravel": "dev-support-laravel-6",
        "zendframework/zend-feed": "^2.12"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "dg/bypass-finals": "^1.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.4.0",
        "phpstan/phpstan-phpunit": "^0.11.2",
        "phpunit/phpunit": "^8.0",
        "roave/no-leaks": "^1.1",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cs-check": "ecs check app domain",
        "cs-fix": "ecs check app domain --fix",
        "my-test" : "phpunit --testsuite Domain",
        "test-coverage": "@php ./build/phpunit --prepend ./build/xdebug-filter.php --coverage-text",
        "test-report": "@php ./build/phpunit --prepend ./build/xdebug-filter.php --coverage-html ./build/report",
        "test-dump": "@php ./build/phpunit --dump-xdebug-filter build/xdebug-filter.php"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/iLexN/graphqlite-laravel.git"
        }
    ]
}
